<h1>User#index</h1>
<p>Find me in app/views/user/index.html.erb</p>


<table>
	<tr>
	<td><strong>Username</strong></td>
	<td><strong>Email</strong></td>
	</tr>	

	<%@users.each do |user| %>
		<tr>
			<td><%= user.username %></td>
			<td><%= user.email %></td>
			<td> <%= link_to 'Editar' , edit_user_path(user) %> </td>
		</tr>

	<% end %>
</table>